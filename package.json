{
  "name": "node-ts-postgres",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "prettier": "node_modules/.bin/prettier --config .prettierrc 'src/**/*.ts' --write",
    "lint": "node_modules/.bin/eslint 'src/*.ts'",
    "build": "rm -rf dist && ttsc && cp -R migrations ./dist/db",
    "dev": "NODE_PATH=src nodemon --watch src -e ts --exec ts-node -r dotenv/config src/index.ts",
    "start": "node dist/index.js",
  },
  "dependencies": {
    "db-migrate": "^0.11.13",
    "db-migrate-pg": "^1.2.2",
    "express": "^4.17.1",
    "pg": "^8.7.1",
    "postgres-migrations": "^5.1.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.11",
    "@types/node": "^8.10.2",
    "@types/pg": "^7.14.11",
    "@types/supertest": "^2.0.11",
    "dotenv": "^9.0.2",
    "eslint": "^8.5.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "nodemon": "^2.0.7",
    "prettier": "^2.5.1",
    "supertest": "^6.1.6",
    "ts-node": "^8.10.2",
    "ttypescript": "^1.5.12",
    "typescript": "^4.2.4",
    "typescript-transform-paths": "^2.2.3"
  }
}
